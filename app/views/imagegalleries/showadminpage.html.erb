        <div class="container  ">
           <div class="d-flex flex-wrap flex-row  justify-content-start ">  
              <% @imagegalleries.each do |imagegallery| %>
              <div class="card mt-4 mx-2" style="width:16rem;">      
                <%=  image_tag(imagegallery.image , class:"onHoverCardImg card-img-top img-fluid " )  %>  
                      <div class="card-body py-0">
                          <p class="card-text card-text-edit mt-2">Title : <%=   imagegallery.title  %> </p>                           
                          <p class="card-text card-caption-edit text-justify "> Caption : <%=  imagegallery.caption  %>  </p>        
                          <p class="card-text card-tag-edit ">                                 
                                  <p>Tags: <%= imagegallery.tag_list %></p>                             
                          </p>                 
                          <p class="card-text d-flex flex-nowrap mt-1 mb-1"><small>Created on  :<%= imagegallery.created_at.to_s.split(' ')[0] %>  </small>  </p>
                          <p class="card-text d-flex flex-nowrap mb-1"><small>Updated on  :<%= imagegallery.updated_at.to_s.split(' ')[0] %>   </small> </p>
                           <p class="card-text card-published-edit"><small>Published By : <%=   imagegallery.user.email.split('@')[0] %> </small></p>  
                      </div>
                      <div class="card-body d-flex  pt-2 ">
                          
                        <a  class="card-link "><%= link_to 'Show', imagegallery %></a>
                        <a  class="card-link flex-fill"><%= link_to 'Edit', edit_imagegallery_path(imagegallery) %></a>
                        <a  class="card-link flex-fill"><%= link_to 'Destroy', imagegallery, method: :delete, data: { confirm: 'Are you sure?' } %></a>
                      </div>               
              </div>
              <% end %>
          </div> 

    </div>
        
        </div>
   
<div class="m-5 d-flex justify-content-center">
   <%= paginate @imagegalleries %>
</div>




      
   
         
                             